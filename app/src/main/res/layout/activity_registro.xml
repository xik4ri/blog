<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F4F4F4"
        tools:context=".Registro">

    <EditText
            android:layout_width="292dp"
            android:layout_height="53dp"
            android:inputType="text"
            android:ems="10"
            android:id="@+id/correoTxt"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="59dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="49dp" android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="19dp"
            app:layout_constraintBottom_toTopOf="@+id/contraTxt" android:hint="correo"/>
    <EditText
            android:layout_width="292dp"
            android:layout_height="53dp"
            android:inputType="text"

            android:ems="10"
            android:id="@+id/contactoTxt"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="59dp"
            android:layout_marginEnd="60dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.554"
            android:hint="contacto"/>
    <EditText
            android:layout_width="292dp"
            android:layout_height="53dp"
            android:inputType="text"
            android:ems="10"
            android:id="@+id/descTxt" android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="59dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="388dp"
            android:layout_marginTop="37dp" app:layout_constraintTop_toBottomOf="@+id/nombreTxt"
            android:hint="descripcion"/>
    <EditText
            android:layout_width="292dp"
            android:layout_height="53dp"
            android:inputType="text"
            android:ems="10"
            android:id="@+id/contraTxt" android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="59dp" android:layout_marginTop="19dp"
            app:layout_constraintTop_toBottomOf="@+id/correoTxt" android:hint="contrasenia"/>
    <EditText
            android:layout_width="292dp"
            android:layout_height="53dp"
            android:inputType="text"
            android:ems="10"
            android:id="@+id/nombreTxt" android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/contraTxt" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="59dp" android:layout_marginEnd="60dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0" android:hint="nombre"/>
    <Button
            android:text="registrarse"
            android:layout_width="wrap_content"
            app:backgroundTint="@null"
            android:layout_height="wrap_content" android:id="@+id/registrarseBtn"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="147dp"
            android:layout_marginEnd="147dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="203dp"
            android:layout_marginTop="51dp" app:layout_constraintTop_toBottomOf="@+id/contactoTxt"
            android:textColorLink="#000000"/>
</androidx.constraintlayout.widget.ConstraintLayout>